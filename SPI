#define F_CPU 160000000L
#include <avr/io.h>
#include <stdio.h>

#include "UART1.h"
#include "EEPROM_25LC010.h"

FILE OUTPUT = FDEV_SETUP_STREAM(UART1_transmit, NULL, _FDEV_SETUP_WRITE);
FILE INPUT = FDEV_SETUP_STREAM(NULL, UART1_receive, _FDEV_SETUP_READ);

int main(void)
{
    /* Replace with your application code */
	
	SPI_Init();
	UART1_init();
	
	stdout = &OUTPUT;
	stdin = &INPUT;
	
	
	EEPROM_write_int2(10, 20000);
	
	
	printf("%d\n\r", EEPROM_read_int(10));
	
    while (1);
	return 0;
}
